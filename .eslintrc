{
	"env": {
		"browser": true,
		"jquery": true,
		"node": true
	},
	"globals": {
		"createjs": true,
		"io": true
	},
	"rules": {
		"no-bitwise": 2,
		"camelcase": 2,
		"curly": 2,
		"indent": [2, "tab", {"VariableDeclarator": 0}],
		"eqeqeq": 2,
		"guard-for-in": 2,
		"no-extend-native": 2,
		"wrap-iife": [2, "any"],
		"no-use-before-define": 2,
		"new-cap": 2,
		"no-caller": 2,
		"no-empty": 2,
		"no-new": 0,
		"quotes": [2,	"single"],
		"no-undef": 2,
		"no-unused-vars": [1, {"vars": "local",	"args": "none"}],
		"strict": 0,
		"semi": 2,
		"no-cond-assign": 0,
		"no-debugger": 2,
		"no-eq-null": 0,
		"no-eval": 0,
		"no-unused-expressions": 0,
		"block-scoped-var": 0,
		"no-iterator": 0,
		"linebreak-style": 0,
		"comma-style": [2, "last"],
		"no-loop-func": 0,
		"no-multi-str": 0,
		"valid-typeof": 0,
		"no-proto": 0,
		"no-script-url": 0,
		"no-shadow": 2,
		"dot-notation": 2,
		"no-new-func": 0,
		"no-new-wrappers": 0,
		"no-invalid-this": 0,

		"space-before-function-paren": [1, "never"],
		"space-return-throw-case": 1,
		"space-before-blocks": 1,
		"space-after-keywords": [1, "always"],
		"space-in-parens": [1, "never"],
		"space-infix-ops": 1,
		"spaced-comment": [1, "always"]
	}
}
